    "def gcd(a,b):\n",
    "    for i in range(min(a,b),0,-1):\n",
    "        if(a%i ==0 and b %i ==0):\n",
    "            return int(a/i),int(b/i),i\n",
    "def solution(w,h):\n",
    "    w1,h1,i=gcd(w,h)\n",
    "    return w*h-(w1+h1-1)*i"
   ]
