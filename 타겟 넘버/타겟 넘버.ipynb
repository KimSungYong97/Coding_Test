    "def solution(numbers, target):\n",
    "    if(target == 0 and not numbers):\n",
    "            return 1\n",
    "    if(target != 0 and not numbers):\n",
    "            return 0\n",
    "    return solution(numbers[1:],target+numbers[0])+solution(numbers[1:],target-numbers[0])"
   ]
